def removecommonwords(sent1,sent2):
    com=[]
    s1 = list(sent1.split())
    s2 = list(sent2.split())
    for i in s1:
        if i in s2:
            s1.remove(i)
            s2.remove(i)
    print(*s1)
    print(*s2)
str1=input("enter the sentence:")
str2=input("enter second sentence:")
removecommonwords(str1,str2)


